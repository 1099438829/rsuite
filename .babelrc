{
  "presets": [
    [
      "env",
      {
        "loose": true
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-proto-to-assign",
    "flow-react-proptypes",
    "transform-react-flow-handled-props",
    "lodash",
    "transform-class-properties",
    "transform-dev"
  ],
  "env": {
    "coverage": {
      "plugins": ["istanbul"]
    }
  }
}
